{
  "name": "nestjs-pracice",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "commit": "git cz",
    "db:migrate": "npm run typeorm migration:run -- -d src/data-access/connection-options.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "seed": "ts-node test/test-helpers/db/seeding.ts",
    "semantic-release": "semantic-release",
    "start": "nest start",
    "start:dev": "nest start --watch | pino-pretty",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "test:cov": "npm run test -- --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:generate": "npm run typeorm migration:generate -- -d src/data-access/connection-options.ts",
    "typeorm:revert": "npm run typeorm migration:revert -- -d src/data-access/connection-options.ts"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/typeorm": "^9.0.1",
    "axios": "^1.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "convict": "^6.2.4",
    "convict-format-with-validator": "^6.2.0",
    "dotenv": "^16.0.3",
    "express-basic-auth": "^1.2.1",
    "nestjs-pino": "^3.2.0",
    "pg": "^8.11.0",
    "pino": "^8.14.1",
    "pino-http": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.6.2",
    "typeorm": "^0.3.16",
    "webpack": "^5.84.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@commitlint/prompt": "^17.6.1",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/release-notes-generator": "^11.0.1",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "commitlint": "^17.6.3",
    "cz-conventional-changelog": "^3.3.0",
    "docker-compose": "^0.24.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security": "^1.7.1",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "is-port-reachable": "3.0.0",
    "jest": "29.5.0",
    "jest-dashboard": "^0.3.1",
    "jest-html-reporters": "^3.1.4",
    "jest-junit": "^12.0.0",
    "jest-watch-master": "^1.0.0",
    "jest-watch-toggle-config": "^2.0.1",
    "jest-watch-typeahead": "^2.2.2",
    "license-checker": "^25.0.1",
    "npm": "^9.6.7",
    "pino-pretty": "^10.0.0",
    "prettier": "^2.3.2",
    "semantic-release": "^21.0.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run format && npm run lint"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
